//q1
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int total = 0;

        for (int i = 0; i < 5; i++) {
            System.out.print("Enter grade " + (i + 1) + ": ");
            int grade = scan.nextInt();
            total += grade;
    System.out.println(total/5);
        }
    }
}
//q2
